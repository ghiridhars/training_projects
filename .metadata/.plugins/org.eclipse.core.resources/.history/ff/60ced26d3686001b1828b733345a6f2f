package com.mindtree.franchise.dto;

public class Team {

	private int id;
	private String name;
	private byte limit;

	public byte getLimit() {
		return limit;
	}

	public void setLimit(byte limit) {
		this.limit = limit;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	@Override
	public String toString() {
		return "Team [id=" + id + ", name=" + name + ", limit=" + limit + "]";
	}

}
